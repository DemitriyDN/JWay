.markdownEditor {
  #markItUpPost_body.markItUp {
    width: 1150px;
    .markItUpContainer {
      background: #fbfbfb url(<%= asset_path 'noise.png' %>) top left;
      .markItUpHeader {
        ul {
          margin: 0px;
          padding-left: 5px;
        }
      }
      .markItUpEditor {
        display: block;
        width: 1120px;
        max-width: 1120px;
        height: 1500px;
        background: #fbfbfb url(<%= asset_path 'noise.png' %>) top left;
        padding: 5px;
        font-size: 15px;
      }
    }
  }
}
